{
	"env":
	{
		"browser": true,
		"worker": true,

		"amd": true,
		"es6": true,

		"jquery": true
	},
	"parserOptions":
	{
		"ecmaVersion": 2018,
		"sourceType": "module"
	},

	"extends": "eslint:recommended",
	"globals":
	{
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"rules":
	{
		"no-empty": "warn",
		"no-unused-vars": "warn"
	}
}
